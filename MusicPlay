game:GetService("StarterGui"):SetCore("SendNotification", { 
	Title = "Load";
	Text = "";
	Icon = ""})
Duration = 15;

-- If you want to edit, just edit.

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "F4S - MusicPlay", HidePremium = false, Intro = true, IntroText = "Have Fun!", SaveConfig = true, ConfigFolder = "MPlayF4S"})

-- Add meow = 3

writefile("meow.png", game:HttpGet("https://github.com/0nee-a/Musicmodel/raw/refs/heads/main/meow.png"))

-- Music Tab

local MusicPlayTab = Window:MakeTab({
	Name = "MPlay",
	Icon = "",
	PremiumOnly = false
})

-- Add sounds

Su = Instance.new("Sound", Workspace)
Su.Name = "SoundT4S"
Su.Looped = false
Su.Volume = 1

Si = Instance.new("Sound", Workspace)
Si.Name = "SoundT4SC"
Si.Looped = false
Si.Volume = 1

local Section = MusicPlayTab:AddSection({
	Name = "Musicplay"
})

MusicPlayTab:AddTextbox({
	Name = "SoundId",
	Default = "",
	TextDisappear = false,
	Callback = function(Value1)
              local mai = game.Workspace.SoundT4S
              mai.SoundId = ("rbxassetid://"..Value1)
	end	  
})

MusicPlayTab:AddTextbox({
	Name = "Volume",
	Default = "1",
	TextDisappear = false,
	Callback = function(Value2)
              local mai2 = game.Workspace.SoundT4S
              mai2.Volume = (Value2)
	end	  
})

MusicPlayTab:AddTextbox({
	Name = "Pitch",
	Default = "1",
	TextDisappear = false,
	Callback = function(Value4)
              local mai4 = game.Workspace.SoundT4S
              mai4.PlaybackSpeed = (Value4)
	end
})
MusicPlayTab:AddButton({
	Name = "Play",
	Callback = function()
      		local mai1 = game.Workspace.SoundT4S
      mai1:Play()
  	end    
})

MusicPlayTab:AddButton({
	Name = "Stop",
	Callback = function()
      		local mai3 = game.Workspace.SoundT4S
      mai3:Stop()
  	end    
})

MusicPlayTab:AddToggle({
	Name = "Looped",
	Default = false,
	Callback = function(Value8)
		local mao1 = game.Workspace.SoundT4S
		mao1.Looped = (Value8)
	end    
})


-- Custom Tab 

local CustomTab = Window:MakeTab({
	Name = "Custom",
	Icon = "rbxassetid://9180622670",
	PremiumOnly = false
})

local Section = CustomTab:AddSection({
	Name = "Custom"
})

CustomTab:AddTextbox({
	Name = "Name",
	Default = ".mp3",
	TextDisappear = false,
	Callback = function(Value5)
              local ai = game.Workspace.SoundT4SC
              ai.SoundId = getcustomasset(Value5)
	end	  
})

CustomTab:AddTextbox({
	Name = "Volume",
	Default = "1",
	TextDisappear = false,
	Callback = function(Value6)
              local ai1 = game.Workspace.SoundT4SC
              ai1.Volume = (Value6)
	end	  
})

CustomTab:AddTextbox({
	Name = "Pitch",
	Default = "1",
	TextDisappear = false,
	Callback = function(Value7)
              local ai2 = game.Workspace.SoundT4SC
              ai2.PlaybackSpeed = (Value7)
	end
})

CustomTab:AddButton({
	Name = "Play",
	Callback = function()
      		local ai3 = game.Workspace.SoundT4SC
      ai3:Play()
  	end    
})

CustomTab:AddButton({
	Name = "Stop",
	Callback = function()
      		local ai4 = game.Workspace.SoundT4SC
      ai4:Stop()
  	end    
})

CustomTab:AddToggle({
	Name = "Looped",
	Default = false,
	Callback = function(Value9)
		local mao2 = game.Workspace.SoundT4SC
		mao2.Looped = (Value9)
	end    
})

-- Not working?Tab

local NotworkingTab = Window:MakeTab({
	Name = "NW?",
	Icon = "",
	PremiumOnly = false
})

local Section = NotworkingTab:AddSection({
	Name = "Not working?"
})

NotworkingTab:AddLabel("Add your song to your exploit's workspace folder!")

-- SettingsTab

local SettingsTab = Window:MakeTab({
	Name = "Settings",
	Icon = "",
	PremiumOnly = false
})

SettingsTab:AddBind({
	Name = "Bind",
	Default = Enum.KeyCode.E,
	Hold = false,
	Callback = function()
		Bind:Set(Enum.KeyCode.E)
	end    
})

SettingsTab:AddButton({
	Name = "Destroy",
	Callback = function()
OrionLib:Destroy()
  	end    
})




task.wait(3)

OrionLib:MakeNotification({
	Name = "Gui",
	Content = "Succes Fully loaded!",
	Image = getcustomasset("meow.png"),
	Time = 7
})

OrionLib:Init()
